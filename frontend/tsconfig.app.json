{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "noEmitOnError": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "../shared/modules/project.ts",
    "src/lang/langStore"
  ]
}